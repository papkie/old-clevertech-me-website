<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="CleverTech homepage">
        <meta name="author" content="CleverTech">
        <title>{{ title }} - {{ subtitle }}</title>
        {% stylesheets 
        '@CleverTechFrontBundle/Resources/public/css/bootstrap.min.css'     
        '@CleverTechFrontBundle/Resources/public/css/style.css'   
        '@CleverTechFrontBundle/Resources/public/css/font-awesome.min.css' 

        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        <link href='http://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body id="page-top" class="index">
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Nawigacja</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand page-scroll" href="#page-top">CleverTech</a>
                </div>
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="hidden">
                            <a href="#page-top"></a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#services">Czym się zajmujemy?</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#portfolio">Portfolio</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#team">O nas</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#contact">Kontakt</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

        <header>
            <div class="container">
                <div class="intro-text">
                    <div class="intro-lead-in">Cześć!</div>
                    <div class="intro-heading">Poznajmy się</div>
                    <a href="#services" class="page-scroll btn btn-xl">Dalej</a>
                </div>
            </div>
        </header>

        {% block content %} {% endblock %}
        
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <span class="copyright">Copyright &copy; CleverTech 2015</span>
                        <ul class="list-inline social-buttons">
                            <li><a href="#"><i class="fa fa-twitter"></i></a>
                            </li>
                            <li><a href="#"><i class="fa fa-facebook"></i></a>
                            </li>
                            <li><a href="#"><i class="fa fa-linkedin"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>

        {% block portfolio_large %}
            {{ render(url("portfolioDetails")) }}
        {% endblock %}
                            
        {% javascripts 
        '@CleverTechFrontBundle/Resources/public/js/jquery.js'        
        '@CleverTechFrontBundle/Resources/public/js/bootstrap.min.js'
        'http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js'
        '@CleverTechFrontBundle/Resources/public/js/classie.js'
        '@CleverTechFrontBundle/Resources/public/js/cbpAnimatedHeader.js'
        '@CleverTechFrontBundle/Resources/public/js/jqBootstrapValidation.js'
        '@CleverTechFrontBundle/Resources/public/js/contact_me.js'
        '@CleverTechFrontBundle/Resources/public/js/main.js'
        '@CleverTechFrontBundle/Resources/public/js/*' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    </body>
</html>
